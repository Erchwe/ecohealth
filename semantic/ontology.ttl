@prefix ex: <https://ecohealth-xi.vercel.app/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

ex:Ontology a owl:Ontology ;
    rdfs:label "EcoHealth Ontology" .

ex:City a rdfs:Class ;
    rdfs:label "City" .

ex:Disease a rdfs:Class ;
    rdfs:label "Disease" .

ex:RiskLevel a rdfs:Class ;
    rdfs:label "Risk level" .

ex:Symptom a rdfs:Class ;
    rdfs:label "Symptom" .

ex:AirQualityObservation a rdfs:Class ;
    rdfs:label "Air quality observation" ;
    rdfs:subClassOf sosa:Observation .

ex:observedCity a rdf:Property ;
    rdfs:label "observed city" ;
    rdfs:domain ex:AirQualityObservation ;
    rdfs:range ex:City .

ex:pm25Value a rdf:Property ;
    rdfs:label "PM2.5 value" ;
    rdfs:domain ex:AirQualityObservation ;
    rdfs:range xsd:float .

ex:unit a rdf:Property ;
    rdfs:label "unit" ;
    rdfs:domain ex:AirQualityObservation ;
    rdfs:range xsd:string .

ex:observedAt a rdf:Property ;
    rdfs:label "observed at" ;
    rdfs:domain ex:AirQualityObservation ;
    rdfs:range xsd:dateTime .

ex:aqiCategory a rdf:Property ;
    rdfs:label "AQI category" ;
    rdfs:domain ex:AirQualityObservation ;
    rdfs:range xsd:string .

ex:hasRiskLevel a rdf:Property ;
    rdfs:label "has risk level" ;
    rdfs:range ex:RiskLevel .

ex:relatedToDisease a rdf:Property ;
    rdfs:label "related to disease" ;
    rdfs:domain ex:RiskLevel ;
    rdfs:range ex:Disease .

ex:triggersRiskFor a rdf:Property ;
    rdfs:label "triggers risk for" ;
    rdfs:domain ex:AirQualityObservation ;
    rdfs:range ex:Disease .

ex:recommendation a rdf:Property ;
    rdfs:label "recommendation" ;
    rdfs:domain ex:AirQualityObservation ;
    rdfs:range xsd:string .

ex:hasName a rdf:Property ;
    rdfs:label "name" ;
    rdfs:domain ex:Disease ;
    rdfs:range xsd:string .

ex:symptoms a rdf:Property ;
    rdfs:label "symptoms" ;
    rdfs:domain ex:Disease ;
    rdfs:range xsd:string .

ex:description a rdf:Property ;
    rdfs:label "description" ;
    rdfs:domain ex:Disease ;
    rdfs:range xsd:string .

ex:hasSymptom a rdf:Property ;
    rdfs:label "has symptom" ;
    rdfs:domain ex:Disease ;
    rdfs:range ex:Symptom .

ex:sensitiveGroup a rdf:Property ;
    rdfs:label "sensitive group" ;
    rdfs:domain ex:Disease ;
    rdfs:range xsd:string .
