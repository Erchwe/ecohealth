@prefix ex: <https://ecohealth-xi.vercel.app/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .


ex:RiskLevel/Low a ex:RiskLevel ; rdfs:label "Low" .
ex:RiskLevel/Medium a ex:RiskLevel ; rdfs:label "Medium" .
ex:RiskLevel/High a ex:RiskLevel ; rdfs:label "High" .
ex:RiskLevel/VeryHigh a ex:RiskLevel ; rdfs:label "Very High" .

<https://ecohealth-xi.vercel.app/Disease/asma> a ex:Disease ;
    rdfs:label "Asma" ;
    ex:description """
      Kondisi peradangan kronis pada saluran napas yang membuat penderitanya sangat sensitif terhadap polusi udara.
    """ ;
    ex:hasRiskLevel <https://ecohealth-xi.vercel.app/RiskLevel/Tinggi> ;
    ex:hasSymptom <https://ecohealth-xi.vercel.app/Symptom/Sesaknapas>,
        <https://ecohealth-xi.vercel.app/Symptom/batuk>,
        <https://ecohealth-xi.vercel.app/Symptom/dadaterasaberat>,
        <https://ecohealth-xi.vercel.app/Symptom/mengi> .

<https://ecohealth-xi.vercel.app/Disease/ispa> a ex:Disease ;
    rdfs:label "ISPA" ;
    ex:description """
      Infeksi mendadak pada saluran napas bagian atas maupun bawah yang sering dipicu atau diperparah oleh kualitas udara yang buruk.
    """ ;
    ex:hasRiskLevel <https://ecohealth-xi.vercel.app/RiskLevel/Sedang> ;
    ex:hasSymptom <https://ecohealth-xi.vercel.app/Symptom/Batuk>,
        <https://ecohealth-xi.vercel.app/Symptom/demam>,
        <https://ecohealth-xi.vercel.app/Symptom/pilek>,
        <https://ecohealth-xi.vercel.app/Symptom/sakittenggorokan> .

<https://ecohealth-xi.vercel.app/Disease/pneumonia> a ex:Disease ;
    rdfs:label "Pneumonia" ;
    ex:description """
      Infeksi pada kantung udara di paru-paru yang dapat diperburuk oleh paparan polusi udara jangka panjang.
    """ ;
    ex:hasRiskLevel <https://ecohealth-xi.vercel.app/RiskLevel/Tinggi> ;
    ex:hasSymptom <https://ecohealth-xi.vercel.app/Symptom/Demam>,
        <https://ecohealth-xi.vercel.app/Symptom/batukberdahak>,
        <https://ecohealth-xi.vercel.app/Symptom/napascepat>,
        <https://ecohealth-xi.vercel.app/Symptom/nyeridada> .

<https://ecohealth-xi.vercel.app/RiskLevel/Sedang> a ex:RiskLevel ;
    rdfs:label "Sedang" .

<https://ecohealth-xi.vercel.app/Symptom/Batuk> a ex:Symptom ;
    rdfs:label "Batuk" .

