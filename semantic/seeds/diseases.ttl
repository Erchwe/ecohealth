    rdfs:label "batuk" .

<https://ecohealth-xi.vercel.app/Symptom/batukberdahak> a ex:Symptom ;
    rdfs:label "batuk berdahak" .

<https://ecohealth-xi.vercel.app/Symptom/dadaterasaberat> a ex:Symptom ;
    rdfs:label "dada terasa berat" .

<https://ecohealth-xi.vercel.app/Symptom/demam> a ex:Symptom ;
    rdfs:label "demam" .

<https://ecohealth-xi.vercel.app/Symptom/mengi> a ex:Symptom ;
    rdfs:label "mengi" .

<https://ecohealth-xi.vercel.app/Symptom/napascepat> a ex:Symptom ;
    rdfs:label "napas cepat" .

<https://ecohealth-xi.vercel.app/Symptom/nyeridada> a ex:Symptom ;
    rdfs:label "nyeri dada" .

<https://ecohealth-xi.vercel.app/Symptom/pilek> a ex:Symptom ;
    rdfs:label "pilek" .

<https://ecohealth-xi.vercel.app/Symptom/sakittenggorokan> a ex:Symptom ;
