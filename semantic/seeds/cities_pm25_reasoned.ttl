<https://ecohealth-xi.vercel.app/Disease/asma> a ex:Disease ;
    rdfs:label "Asma" ;
    ex:description """
      Kondisi peradangan kronis pada saluran napas yang membuat penderitanya sangat sensitif terhadap polusi udara.
    """ ;
    ex:hasRiskLevel <https://ecohealth-xi.vercel.app/RiskLevel/Tinggi> ;
    ex:hasSymptom <https://ecohealth-xi.vercel.app/Symptom/Sesaknapas>,
        <https://ecohealth-xi.vercel.app/Symptom/batuk>,
        <https://ecohealth-xi.vercel.app/Symptom/dadaterasaberat>,
        <https://ecohealth-xi.vercel.app/Symptom/mengi> .

<https://ecohealth-xi.vercel.app/Disease/ispa> a ex:Disease ;
    rdfs:label "ISPA" ;
    ex:description """
      Infeksi mendadak pada saluran napas bagian atas maupun bawah yang sering dipicu atau diperparah oleh kualitas udara yang buruk.
    """ ;
    ex:hasRiskLevel <https://ecohealth-xi.vercel.app/RiskLevel/Sedang> ;
    ex:hasSymptom <https://ecohealth-xi.vercel.app/Symptom/Batuk>,
        <https://ecohealth-xi.vercel.app/Symptom/demam>,
        <https://ecohealth-xi.vercel.app/Symptom/pilek>,
        <https://ecohealth-xi.vercel.app/Symptom/sakittenggorokan> .

